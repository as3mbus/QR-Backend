{
  "name": "OutletCode",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "code-id": {
      "type": "string",
      "required": true
    },
    "outlet-id": {
      "type": "number",
      "required": true
    },
    "used-date": {
      "type": "date",
      "required":true,
      "default":"$now"
    }
  },
  "validations": [],
  "relations": {
    "voucherCode": {
      "type": "belongsTo",
      "model": "VoucherCode",
      "foreignKey": "code-id"
    },
    "outlet": {
      "type": "belongsTo",
      "model": "Outlet",
      "foreignKey": "outlet-id"
    }
    
  },
  "acls": [],
  "methods": {}
}
